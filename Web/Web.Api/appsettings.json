{
  "AllowedHosts": "*",
  "Configuration": {
    "ConnectionStrings": {
      "DefaultConnection": "Server=;Uid=;Pwd=;Database=;MultipleActiveResultSets=true;TrustServerCertificate=True;"
    },
    "UseLogger": true,
    "Logger": {
      "ConnectionString": "Server=;Uid=;Pwd=;Database=;MultipleActiveResultSets=true;TrustServerCertificate=True;",
      "ColumnOptions": {
        "RemoveStandardColumns": [ "MessageTemplate", "Properties" ],
        "TimeStamp": {
          "ColumnName": "Timestamp",
          "ConvertToUtc": false
        },
        "Id": {
          "DataType": "bigint"
        }
      },
      "AutoCreateTable": true,
      "SchemaName": "dbo",
      "TableName": "Log"
    }
    
  }
}
